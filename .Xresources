! URxvt settings

! Set font
URxvt.font: xft:Noto Sans Mono:size=13

! Set transparency
URxvt*inheritPixmap: true
URxvt*transparent: true
URxvt*shading: 50

! Remove scrollbar
URxvt*scrollBar: false
